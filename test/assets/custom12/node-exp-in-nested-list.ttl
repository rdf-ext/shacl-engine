@prefix ex: <http://example.org/>.
@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix shn: <https://schemas.link/shacl-next#>.
@prefix sht: <http://www.w3.org/ns/shacl-test#>.

<> a mf:Manifest;
  mf:entries (<node-exp-in-nested-list>).

ex:data
  ex:property 0E0.

ex:PropertyShape a sh:PropertyShape;
  sh:in [
      shn:subtract (3E0 _:b1)
    ];
  sh:path ex:property.

ex:RootShape a sh:NodeShape;
  sh:property ex:PropertyShape;
  sh:targetNode ex:data.

<node-exp-in-nested-list> a sht:Validate;
  rdfs:label "Test of sh:in node expression with nested list arguments";
  mf:action [
      sht:dataGraph <>;
      sht:shapesGraph <>
    ];
  mf:result [ a sh:ValidationReport;
      sh:conforms true;
      sh:result [ a sh:ValidationResult;
          sh:focusNode ex:data;
          sh:resultPath ex:property;
          sh:resultSeverity shn:Debug;
          sh:sourceConstraintComponent sh:InConstraintComponent;
          sh:sourceShape ex:PropertyShape;
          sh:value 0E0
        ], [ a sh:ValidationResult;
          sh:focusNode ex:data;
          sh:resultPath ex:property;
          sh:resultSeverity shn:Trace;
          sh:sourceConstraintComponent shn:TraversalConstraintComponent;
          sh:sourceShape ex:PropertyShape;
          sh:value 0E0
        ]
    ].

_:b1
  shn:add (2E0 1E0).
