@prefix ex: <http://example.org/>.
@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix shn: <https://schemas.link/shacl-next#>.
@prefix sht: <http://www.w3.org/ns/shacl-test#>.

<> a mf:Manifest;
  mf:entries (<node-exp-in-gte>).

ex:data
  ex:property1 true;
  ex:property2 false;
  ex:property3 true.

ex:Property1Shape a sh:PropertyShape;
  sh:in [
      shn:gte (1 1)
    ];
  sh:path ex:property1.

ex:Property2Shape a sh:PropertyShape;
  sh:in [
      shn:gte (1 2)
    ];
  sh:path ex:property2.

ex:Property3Shape a sh:PropertyShape;
  sh:in [
      shn:gte (2 1)
    ];
  sh:path ex:property3.

ex:RootShape a sh:NodeShape;
  sh:property
    ex:Property1Shape,
    ex:Property2Shape,
    ex:Property3Shape;
  sh:targetNode ex:data.

<node-exp-in-gte> a sht:Validate;
  rdfs:label "Test of sh:in node expression with shn:gte";
  mf:action [
      sht:dataGraph <>;
      sht:shapesGraph <>
    ];
  mf:result [ a sh:ValidationReport;
      sh:conforms true;
      sh:result [ a sh:ValidationResult;
          sh:focusNode ex:data;
          sh:resultPath ex:property1;
          sh:resultSeverity shn:Trace;
          sh:sourceConstraintComponent shn:TraversalConstraintComponent;
          sh:sourceShape ex:Property1Shape;
          sh:value true
        ], [ a sh:ValidationResult;
          sh:focusNode ex:data;
          sh:resultPath ex:property1;
          sh:resultSeverity shn:Debug;
          sh:sourceConstraintComponent sh:InConstraintComponent;
          sh:sourceShape ex:Property1Shape;
          sh:value true
        ], [ a sh:ValidationResult;
          sh:focusNode ex:data;
          sh:resultPath ex:property2;
          sh:resultSeverity shn:Debug;
          sh:sourceConstraintComponent sh:InConstraintComponent;
          sh:sourceShape ex:Property2Shape;
          sh:value false
        ], [ a sh:ValidationResult;
          sh:focusNode ex:data;
          sh:resultPath ex:property2;
          sh:resultSeverity shn:Trace;
          sh:sourceConstraintComponent shn:TraversalConstraintComponent;
          sh:sourceShape ex:Property2Shape;
          sh:value false
        ], [ a sh:ValidationResult;
          sh:focusNode ex:data;
          sh:resultPath ex:property3;
          sh:resultSeverity shn:Trace;
          sh:sourceConstraintComponent shn:TraversalConstraintComponent;
          sh:sourceShape ex:Property3Shape;
          sh:value true
        ], [ a sh:ValidationResult;
          sh:focusNode ex:data;
          sh:resultPath ex:property3;
          sh:resultSeverity shn:Debug;
          sh:sourceConstraintComponent sh:InConstraintComponent;
          sh:sourceShape ex:Property3Shape;
          sh:value true
        ]
    ].
